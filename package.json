{
  "name": "qa-control",
  "description": "QA control of things that nobody controls",
  "version": "0.3.0",
  "author": "Codenautas <codenautas@googlegroups.com>",
  "license": "MIT",
  "repository": "codenautas/qa-control",
  "contributors": [{
    "name": "Emilio Platzer", "email": "emilioplatzer@gmail.com"
  },{
    "name": "Diego Florio", "email": "diegoefe@gmail.com"
  }],
  "bin": {
    "qa-control": "./bin/qa-control-run.js"
  },
  "main": "./bin/qa-control.js",
  "dependencies": {
    "commander": "~2.9.0",
    "eslint": "~2.10.0",
    "fs-extra": "~0.30.0",
    "fs-promise": "~0.5.0",
    "js-yaml": "~3.6.0",
    "jshint280": "~1.0.0",
    "jshint": "~2.9.1",
    "lodash": "~4.12.0",
    "parameters": "~0.1.5",
    "semver": "~5.1.0",
    "strip-bom-string": "~0.1.0",
    
    "best-promise": "~0.2.4",
    "multilang": "~1.0.11"
  },
  "devDependencies": {
    "expect.js": "~0.3.1",
    "istanbul": "~0.4.3",
    "mocha": "~2.4.5",
    "sinon": "~1.17.3",

    "expect-called": "~0.4.0"
  },
  "engines": {
    "node": ">= 0.12"
  },
  "scripts": {
    "test": "mocha --reporter spec --bail --check-leaks test/",
    "test-ci": "istanbul cover node_modules/mocha/bin/_mocha --report lcovonly -- --reporter spec --check-leaks test/",
    "test-cov": "istanbul cover node_modules/mocha/bin/_mocha -- --reporter dot --check-leaks test/",
    "start": "node example/server.js"
  },
  "files": [
    "bin"
  ],
  "jshintConfig": {
    "asi": false,
    "forin": true,
    "curly": true,
    "esversion": 6
  },
  "eslintConfig":{
    "env": {
      "node": false
    },
    "rules": {
      "strict": 0,
      "no-console": 1,
      "no-unused-vars": 1
    },
    "parserOptions": {
      "ecmaVersion": 6
    }
  },
  "qa-control": {
    "package-version": "0.2.0",
    "coverage": 90,
    "run-in": "server",
    "stability": "extending",
    "type": "cmd-tool",
    "ecmaVersion": 6
  }
}
