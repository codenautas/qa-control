{
  "name": "qa-control",
  "description": "QA control of things that nobody controls",
  "version": "0.4.19",
  "author": "Codenautas <codenautas@googlegroups.com>",
  "license": "MIT",
  "repository": "codenautas/qa-control",
  "contributors": [
    {
      "name": "Emilio Platzer",
      "email": "emilioplatzer@gmail.com"
    },
    {
      "name": "Diego Florio",
      "email": "diegoefe@gmail.com"
    }
  ],
  "bin": {
    "qa-control": "./bin/qa-control-run.js"
  },
  "main": "./bin/qa-control.js",
  "dependencies": {
    "commander": "~3.0.0",
    "eslint": "~6.2.1",
    "fs-extra": "~8.1.0",
    "fs-promise": "~2.0.3",
    "js-yaml": "~3.13.1",
    "jshint": "~2.10.2",
    "parameters": "~0.6.0",
    "semver": "~6.3.0",
    "strip-bom-string": "~1.0.0",
    "best-globals": "~0.10.22",
    "multilang": "~1.0.23"
  },
  "devDependencies": {
    "expect.js": "~0.3.1",
    "istanbul": "~0.4.5",
    "mocha": "~6.2.0",
    "sinon": "~7.4.1",
    "expect-called": "~0.4.0"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "test": "mocha --reporter spec --bail --check-leaks test/",
    "test-nc": "mocha --reporter spec --bail --check-leaks --no-colors test/",
    "test-ci": "istanbul cover node_modules/mocha/bin/_mocha --report lcovonly -- --reporter spec --check-leaks test/",
    "test-cov": "istanbul cover node_modules/mocha/bin/_mocha -- --reporter dot --check-leaks test/",
    "start": "node example/server.js",
    "report": "istanbul report",
    "qac": "node bin/qa-control-run.js . -v",
    "all": "npm test && npm run test-cov && npm run report && npm run qac"
  },
  "files": [
    "bin"
  ],
  "qa-control": {
    "package-version": "0.3.0",
    "coverage": 90,
    "run-in": "server",
    "stability": "extending",
    "type": "cmd-tool",
    "ecmaVersion": 6
  }
}
